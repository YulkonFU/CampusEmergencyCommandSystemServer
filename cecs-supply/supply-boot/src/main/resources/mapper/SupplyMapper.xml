<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.cupk.supply.mapper.SupplyMapper">
    <select id="getRemaining" resultType="cn.edu.cupk.supply.entity.dto.SupplyAmountDTO">
        SELECT
            supply_name AS name,
            inventory   AS amount
        FROM
            supply
        WHERE
            deleted = 0;
    </select>
</mapper>